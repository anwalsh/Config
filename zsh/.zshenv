export RUST="$HOME/.cargo/bin"
export FZF="$HOME/.fzf/bin"
export ZSH="$HOME/.oh-my-zsh"
export NPATH="$HOME/n/bin"
export LOCALBIN="/usr/local/bin"
export LOCALSBIN="/usr/local/sbin"
export BIN="/usr/bin"
export SBIN="/usr/sbin"
export TBIN="/sbin:/bin"
export GOROOT="/usr/lib/go"
export GOPATH="$HOME/go"
export GOBIN="$GOPATH/bin"
export T2PATH="$HOME/t2"
export JAVA_HOME="/usr/lib/jvm/default"
export ANT_HOME="/opt/ant"
export ANT_BIN="$ANT_HOME/bin"
export ANT_OPTS="-Xms64m -Xmx1500m"
export PATH="$SBIN:$BIN:$TBIN:$LOCALBIN:$LOCALSBIN:$RUST:$NPATH:GOROOT:$GOBIN:$GOPATH:$T2PATH:$JAVA_HOME:$ANT_HOME:$ANT_BIN:$FZF"
