export RUST="$HOME/.cargo/bin"
export RUST_ENV="$HOME/.cargo/env"
export FZF="$HOME/.fzf/bin"
export ZSH="$HOME/.oh-my-zsh"
export NPATH="$HOME/n/bin"
export LOCALBIN="/usr/local/bin"
export LOCALSBIN="/usr/local/sbin"
export BIN="/usr/bin"
export SBIN="/usr/sbin"
export TBIN="/sbin:/bin"
export GOROOT="/usr/lib/go"
export GOPATH="$HOME/go"
export GOBIN="$GOPATH/bin"
export T2PATH="$HOME/t2"
export JAVA_HOME="/usr/lib/jvm/default"
export ANT_HOME="/opt/ant"
export ANT_BIN="$ANT_HOME/bin"
export ANT_OPTS="-Xms64m -Xmx1500m"
export CODE="/usr/sbin/code-insiders"
export NODEMODS="$HOME/node_modules/.bin"
export N="$HOME/n/bin"
export LOCAL="$HOME/.local/bin"
export KUBECONFIG="$HOME/.kube/config"
export TOOLS="$HOME/tools"
export SNAP="/snap/bin"
export KREW_ROOT="$HOME/.krew"
export KREW="$KREW_ROOT/bin"
export CODE="/usr/share/code/bin"
export PATH="$RUST:$TOOLS:$SBIN:$BIN:$TBIN:$LOCALBIN:$LOCALSBIN:$NPATH:$GOROOT:$GOBIN:$GOPATH\
:$T2PATH:$JAVA_HOME:$ANT_HOME:$ANT_BIN:$FZF:$CODE:$NODEMODS:$N:$LOCAL:$KUBECONFIG:$SNAP:$KREW_ROOT\
:$KREW:$RUST_ENV:$CODE:$PATH"
